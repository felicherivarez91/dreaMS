    <string name="challenge_dreams_vision_acuity">Vision acuity</string>
    <string name="challenge_dreams_vision_acuity_description">Keep the phone comfortably 40cm from your eyes (phone will guide you). Then swipe with your finger in the direction of the letter E: up, down, left or right.</string>
    <string name="challenge_dreams_vision_contrast">Vision contrast</string>
    <string name="challenge_dreams_vision_contrast_description">Keep the phone comfortably 40cm from your eyes (phone will guide you). Then swipe with your finger in the direction of the letter C: up, down, left or right.</string>
    <string name="challenge_dreams_scale_msis">MSIS-29</string>
